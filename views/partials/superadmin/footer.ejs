</div>
</div>

<script>
  function navbarActive() {
    let pathname = window.location.pathname;
    let pathParts = pathname.split('/');
    let currentPage = pathParts[2];
    document.querySelector('.nav-item-' + currentPage).classList.add('nav-item-active')
    document.querySelector('.nav-link-' + currentPage).classList.add('nav-link-active')
    document.getElementById(currentPage).src = "/" + currentPage + "(active).svg"
  }
  navbarActive();
</script>

<!-- Bootstrap JS -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
