			<script>
				function showContents(contentName, redirectionName) {
					const content = document.getElementById(contentName);
					const redirection = document.getElementById(redirectionName);
					if (content.style.display === 'none' || content.style.display === '') {
						content.style.display = 'block'
						redirection.innerText = '<'
					} else {
						content.style.display = 'none'
						redirection.innerText = '>'
					}
				}
				document.onclick = function (event) {
					const contentCategory = document.getElementById('contentCategory');
					const contentAccount = document.getElementById('contentAccount');
					const redirectionCategory = document.getElementById('redirectionCategory');
					const redirectionAccount = document.getElementById('redirectionAccount');

					if (
						!contentCategory.contains(event.target) &&
						event.target.id !== 'btnCategory' &&
						contentCategory.style.display === 'block'
					) {
						contentCategory.style.display = 'none';
						redirectionCategory.innerText = '>';
					}

					if (
						!contentAccount.contains(event.target) &&
						event.target.id !== 'btnAccount' &&
						contentAccount.style.display === 'block'
					) {
						contentAccount.style.display = 'none';
						redirectionAccount.innerText = '>';
					}
				};
			</script>
			